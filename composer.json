{
    "name":"skrtdev/novagram",
    "description":"An Object-Oriented PHP Library for Telegram Bots",
    "license":"MIT",
    "authors":[
        {
            "name":"Gaetano Sutera",
            "email":"gaetanosutera@yahoo.it",
            "role":"Developer"
        }
    ],
    "require":{
        "php":">=7.4",
        "ext-json":"*",
        "ext-mbstring":"*",
        "ext-curl":"*",
        "skrtdev/async":"dev-master",
        "monolog/monolog":"^2.1"
    },
    "autoload": {
        "psr-4":{
            "skrtdev\\Telegram\\": "src/Telegram/types",
            "skrtdev\\": "src/"
        },
        "classmap":[
            "src/bc",
            "src/proto",
            "src/traits",
            "src/NovaGram/parser.php"
        ]
    }
}
